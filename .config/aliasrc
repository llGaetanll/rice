#!/bin/sh

alias nrd="npm run dev"
alias ns="npm start"
alias py3="python3"
alias gs="git add"
alias gs="git status"
alias gp="git push"
alias gc="git checkout"
alias gb="git branch"
alias dcu="docker-compose up"
alias dcd="docker-compose down"
alias synctime="sudo ntpdate pool.ntp.org"
alias tmux="tmux -u"
alias lg="lazygit"
alias tdl="yt-dlp -f mp4 -o \"$HOME/desktop/%(id)s.%(ext)s\""
alias ydl="yt-dlp -o \"$HOME/Videos/downloads/%(id)s.%(ext)s\""
alias ytthumbnail="yt-dlp --write-thumbnail --convert-thumbnails jpg --skip-download"
